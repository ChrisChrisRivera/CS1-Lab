sentence= "can a fish fish a fish in a fish can"


def find_number_of_words(sentence,word_to_count):
  number_of_word=0
  words=sentence.split()
  for word in words:
    if(word==word_to_count):
      number_of_word+=1
  return number_of_word

print(find_number_of_words(sentence,"can"))

product_ids=["asd3","efg67","rty678","bfd32"]
search= input("enter id: ")
if(search in product_ids):
  print("we have it")
else:
  print("we do not have")

def func1(*args):
  for i in args:
    print(i)

func1(20,40,60)
func1(80,100)

def calculateSum(num):
  if num:
    return num + calculateSum(num-1)
  else:
    return 0
res = calculateSum(10)
print(res)

def printEveIndexChar(str):
  for i in range(0, len(str)-1, 2):
    inputStr = input("Enter String ")
    print("Printing only even index chars")(printEveIndexChar(inputStr))

def isFirst_And_Last_Same(numberList):
  firstElement = numberList[0]
  lastElement = numberList[-1]
  if(firstElement == lastElement):
    return True
  else:
    return False
numList = [10, 20, 30, 40, 10]
print("The first and last number of a list is same")
print("result is", isFirst_And_Last_Same(numList))